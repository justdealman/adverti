<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http:/www.mozilla.org/2006/browser/search/">
    <ShortName>Поиск сувениров</ShortName>
    <Description>http://www.adverti.ru</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="26" height="16" type="image/png">
        data:image/png;base64,
    </Image>
    <Url type="text/html" method="get" template="http://www.adverti.ru/catalogsearch/result">
        <Param name="q" value="{searchTerms}"/>
    </Url>
    <Url type="application/opensearchdescription+xml"
         rel="self"
         template="https://support.mozilla.org/ru/search/xml"/>
    <Url type="application/x-suggestions+json"
         template="http://www.adverti.ru/catalogsearch/result/?q={searchTerms}"/>
    <moz:SearchForm>http://www.adverti.ru/</moz:SearchForm>
</OpenSearchDescription>

